@model ManageUsersViewModel

<div class="section section-admin">
    <h6>Manage Users</h6>
        @for (int i = 0; i < this.Model.Users.Count(); i++)
        {
            <partial name="UserSummary" model="@this.Model.Users[i]" />
        }

        <div class="button-group pp-button-bar">
            <button class="btn btn-primary pp-button" type="submit">Submit</button>
            <button class="btn pp-button pp-button-secondary" type="reset">Reset</button>
        </div>
</div>
